<%- include ../partials/header.ejs %>

  <div class="container">
    <br>
    <h3 class="text-center">My Contacts</h3>
    <div class="row justify-content-center">
      <div class="col-md-6 col-md-offset-3 ">

        <br>
        <table class="table label-bordered table-striped table-hover">
          <tr>
            <th class="text-center">First Name</th>
            <th class="text-center">Last Name</th>
            <th class="text-center">Age</th>
          </tr>

          <% contacts.forEach(contact => { %>
            <tr>
              <td class="text-center"><%= contact.firstName %></td>
              <td class="text-center"><%= contact.lastName %></td>
              <td class="text-center"><%= contact.age %></td>
            </tr>
            <% }); %>
        </table>

      </div>
    </div>
  </div>


<%- include ../partials/footer.ejs %>